{
  "name": "marching-cubes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run tailwind && vite",
    "build": "tsc && npm run tailwind:build && vite build",
    "preview": "vite preview",
    "tailwind": "npx tailwindcss -i ./styles/styles.css -o ./styles/tailwind.css",
    "tailwind:dev": "npx tailwindcss -i ./styles/styles.css -o ./styles/tailwind.css --watch",
    "tailwind:build": "npx tailwindcss -i ./styles/styles.css -o ./styles/tailwind.css --minify"
  },
  "devDependencies": {
    "@types/three": "^0.143.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "simplex-noise": "^4.0.0",
    "stats.js": "^0.17.0",
    "three": "^0.143.0",
    "three-noise": "^1.1.2",
    "web-worker": "^1.2.0"
  }
}
